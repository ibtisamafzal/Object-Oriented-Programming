package task_1;

public class Main {
public static void main(String[] args) {
// TODO Auto-generated method stub
	
	for (int i = 2; i <= 100; i++){
		int temp=0;
    	for (int j = 2; j <= i-1; j++) {
    		if (i % j == 0){		
    			temp=temp+1;		
                }
            }
    	if (temp == 0) {
    		int sum=0;
    		sum=sum+i;
    		System.out.print("Sum of First 100 Prime Numbers: " sum);
            }
	}  
    }
}

			

        
