package task_5;

public class Sum_Equals_Third_Integer {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int num1=20, num2=40, num3=60, sum;
		sum = num1+num2;
		if (sum==num3) {
			System.out.print("The sum of Two Integers = Third Integer");
		}
		else
		{
			System.out.print("The sum of Two Integers != Third Integer");
		}
	}

}
